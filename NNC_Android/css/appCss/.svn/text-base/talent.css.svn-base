* {
	margin: 0;
	padding: 0;
}

ul:before,
ul:after {
	height: 0!important;
}

body {
	color: #222;
}

header {
	-webkit-box-shadow: none !important;
	;
	box-shadow: none !important;
}

.banner {
	position: relative;
	background: url(../../image/rank/my_banner.png) center no-repeat;
	height: 9.5rem;
	width: 100%;
	background-size: cover;
	background-position: 0 0;
}

.banner-txt {
	position: absolute;
	left: 1.1rem;
	bottom: 0;
}

.banner-txt p {
	color: #fff;
	float: left;
}

.banner p.total-weight {
	border-right: 1px solid #fff;
	padding-right: .9rem;
	margin-right: .9rem;
}

.rank_bannner {
	background-color: #FFFFFF;
	height: 9rem;
}

.rank-title,
.rank-main,
.rank-bom {
	width: 90%;
	margin: 0 auto;
	border-bottom: 1px solid #D1D1D1;
	color: #29C49C;
}

.rank-title {
	height: 1.7rem;
	text-align: center;
	font-size: .94rem;
	line-height: 1.7rem;
}

.rank-main {
	position: relative;
	height: 5.58rem;
}

.rm-imgborder,
.case-imgborder {
	position: absolute;
	top: 50%;
	margin-top: -2rem;
	left: .94rem;
	width: 4rem;
	height: 4rem;
	border-radius: 50%;
}

.rm-img,
.case-img {
	width: 100%;
	height: 100%;
	border-radius: 50%;
	background: url(../../image/hend_img.png) center no-repeat;
	background-size: cover;
}

.rm-msgbox {
	position: absolute;
	left: 5.6rem;
	width: 60%;
	height: 100%;
}

.rm-msgbox-lf,
.rm-msgbox-rt {
	font-size: .82rem;
	padding: 1.3rem 0;
}

.rm-msgbox-lf {
	float: left;
}

.rm-msgbox-rt {
	width: 3.5rem;
	height: 100%;
	float: right;
}

.nickname {
	margin-bottom: .8rem;
	color: #222222;
}

.rm-rt-totle {
	margin-bottom: .8rem;
}

.rm-rt-kg {
	text-align: center;
}

.rank-bom {
	padding-right: 1rem;
	height: 1.7rem;
	text-align: right;
	font-size: .82rem;
	line-height: 1.7rem;
	border-bottom: 0;
}

.champion-list,
.case-list {
	margin-top: .94rem;
}

.champion-list li,
.case-list li {
	width: 100%;
	position: relative;
	height: 5.88rem;
	line-height: 5.88rem;
	padding: 0;
}

.champion-list li p.mui-navigate-right,
.case-list li p.mui-navigate-right {
	position: absolute;
	display: block;
	right: 0rem;
	top: 0rem;
	width: 3rem;
	height: 100%;
	font-size: 2.5rem;
	color: #999;
}

.champion-list li p.mui-navigate-right:after,
.case-list li p.mui-navigate-right:after {
	color: #999999;
}

.cha-img {
	position: absolute;
	left: 2rem;
	top: 50%;
	margin-top: -1.75rem;
	display: block;
	width: 3.5rem;
	height: 3.5rem;
	border-radius: 50%;
	background: url(../../image/hend_img.png) center no-repeat;
	background-size: cover
}

.cha-msg,
.case-msg {
	position: absolute;
	left: 7.4rem;
	height: 100%;
	font-size: 1rem;
}

.cha-msg p {
	float: left;
	font-size: 1rem;
	color: #222;
}

.cha-msg p.mui-ellipsis {
	margin-left: .82rem;
	color: #999;
	font-size: 1rem;
	;
}

.cha-logo {
	position: absolute;
	left: .3rem;
	top: 0rem;
	width: 1.76rem;
	height: 3.35rem;
	background: url(../../image/rank/rank1.png) center no-repeat;
	background-size: 95%;
}
.cha-logo1 {
	position: absolute;
	left: 15.3rem;
	top: 0rem;
	width: 1.76rem;
	height: 3.35rem;
	background: url(../../image/rank/rank2.png) center no-repeat;
	background-size: 95%;
}
.cha-logo2 {
	position: absolute;
	left: 17.3rem;
	top: 0rem;
	width: 1.76rem;
	height: 3.35rem;
	background: url(../../image/rank/rank3.png) center no-repeat;
	background-size: 95%;
}
.case-imgborder {
	left: 2rem;
}

.case-img {
	background: url(../../image/typical_case.png) center no-repeat;
	background-size: cover;
}

.no_login {
	background: url(../../image/rank/my_gray.png) center no-repeat #fff;
	background-size: 50%;
}
.no_bgcolor{
	background: linear-gradient(to bottom right, #909395, #909395);
}
.isParticipation {
	background-color: #909395;
	display: none;
	height: 1.5rem;
	line-height: 1.5rem;
	text-align: center;
	font-size: 1rem;
	color: #FCB819;
}


/**ranking***/

#refreshContainer {
	background: #fff;
	margin-top: 48px;
	padding: 0;
}

#refreshContainer .newsList {
	padding: 0;
}

#newsListUl li {
	height: 4.7rem;
	border-bottom: 1px solid #c6c6c6;
	padding: 0 1rem;
}

.rank_ground_floor {
	height: 100%
}

.rank_ordinal_number_img,
.rank_ordinal_number {
	float: left;
	width: 1.76rem;
	height: 3.35rem;
	text-align: center;
	line-height: 3.35rem;
	font-size: 1.17rem;
	color: #222;
}

.rank_ordinal_number {
	height: 100%;
	line-height: 4.7rem;
}

.rank_head_img {
	float: left;
	margin-top: 1.1rem;
	margin-left: .82rem;
	width: 2.5rem;
	height: 2.5rem;
	border-radius: 50%;
	background: url(../../image/hend_img.png) center no-repeat;
	background-size: cover;
}

.rank_nick_name {
	float: left;
	width: 26.5%;
	/*min-width: 5.5rem;*/
	height: 100%;
	line-height: 4.7rem;
	text-align: left;
	margin: 0 .6rem;
	white-space: nowrap;
	overflow: hidden;
	font-size: .82rem;
}

.rank_weight {
	padding: 1rem 0rem;
	float: left;
	width: 36.7%;
	height: 100%;
	text-align: left;
	white-space: nowrap;
	overflow: hidden;
}

.rank_weight p {
	font-size: .82rem;
	color: #222;
	margin-bottom: -0.1rem;
}

#newsListUl li:first-child .rank_weight .rank_weight_total,
#newsListUl li:nth-child(2) .rank_weight .rank_weight_total,
#newsListUl li:nth-child(3) .rank_weight .rank_weight_total {
	color: #1ac50d;
}

#newsListUl li:first-child .rank_ordinal_number_img {
	background: url(../../image/rank/rank1.png) center no-repeat;
	background-size: 95%;
}

#newsListUl li:nth-child(2) .rank_ordinal_number_img {
	background: url(../../image/rank/rank2.png) center no-repeat;
	background-size: 95%;
}

#newsListUl li:nth-child(3) .rank_ordinal_number_img {
	background: url(../../image/rank/rank3.png) center no-repeat;
	background-size: 95%;
}

.fontmax {
	font-size: 1rem;
	font-weight: 700;
}