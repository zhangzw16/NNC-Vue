<!doctype html>
<html>

	<head>
		<meta charset="UTF-8">
		<title></title>
		<meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
		<link href="../../css/mui.css" rel="stylesheet" />
		<link rel="stylesheet" href="../../css/labelauty/jquery-labelauty1.css" />
	</head>
	<body style="background-color: #FFFFFF;">
		<div style="text-align: center;">血脂</div>
		<div style="float: left;margin-top: 10px;">
			<input id="2" type="radio" name="bf" value="2" data-labelauty="正常">
		</div>
		<div style="float: left;margin-left: 5%;margin-top: 10px;">
		    <input id="1" type="radio" name="bf" value="1" data-labelauty="偏低">
		</div>
		<div style="float: left;margin-left:5%;margin-top: 10px;">
		    <input id="3" type="radio" name="bf" value="3" data-labelauty="偏高">
		</div>
		<script src="../../js/mui.min.js"></script>
		<script type="text/javascript" src="../../js/jquery.min.js" ></script>
		<script type="text/javascript" src="../../js/jquerylabelauty.js" ></script>
		<script type="text/javascript">
			$(function(){
				$(':input').labelauty();
				var uid = localStorage.getItem("NNC_userInfoId");
				$.ajax({
      				type : "POST",
      				data : {'userInfoId':uid},
      				url  : URL+'app_rest_ful/select_userInfo',
      				dataType : 'json',
      				success : function(msg){
      					if(msg.success==true){
      						if(msg.data.bloodFat == 1){
      							$("#1").attr("checked","checked");
      						}else if(msg.data.bloodFat == 2){
      							$("#2").attr("checked","checked");
      						}else{
      							$("#3").attr("checked","checked");
      						}
      						
      					}
      				}
      			});
			});
			mui.init();
			function getBf(){
				var bf = $.trim($("input[name='bf']:checked").val());
				return bf;
			}
		</script>
	</body>

</html>