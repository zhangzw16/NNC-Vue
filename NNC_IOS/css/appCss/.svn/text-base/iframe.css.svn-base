}

.msgbox-bottom {
	width: 100%;
	margin: 0;
	height: 2.13333333rem;
	line-height: 2.13333333rem;
	position: absolute;
	bottom: 0;
}

.msgbox-bottom button {
	text-align: center;
	width: 49.8%;
	height: 100%;
	background: #21c9ff;
	float: left;
	color: #fff;
	margin: 0;
	padding: 0;
	font-size: 1rem !important;
	border: none;
	border-radius: 0;
}

.msgbox-bottom button:last-child {
	float: right;
}

textarea {
