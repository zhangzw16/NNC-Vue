.displayNone{
	display: none;
}
header{
	-webkit-box-shadow: none !important;;
    box-shadow: none !important;
}
.about-head{
	height: 10em;
	text-align: center;
}
ul.about-list li{
	width: 100%;
}
.mui-table-view-cell:after{
	left: 0;
	color: #efeff5;
}
ul.about-list li a{
	float:left;
	color: #222 !important;
}
ul.about-list li span{
	float:right;
	color: #999;
}
.mui-table-view-chevron .mui-table-view-cell{
	padding-right: 15px;
}
.wx-logo{
	position: relative;
}
.wx-logo:before{
	position: absolute;
	left: -2.8em;
	top:-.6em;
	content: "";
	width:2.4em;
	height:2.4em;
	background: url(../../image/logo1.png) center no-repeat;
	background-size: 70%;
}
a.about-share{
	width: 100%;
	padding: 0 !important;
	margin: 0 !important;
}
.about-share.mui-navigate-right:after{
	color: #999;
	right:0px;
}
footer{
	width: 100%;
	position: fixed;
	bottom: 15px;
	
}
footer p{
	margin: 0;
	width: 100%;
	text-align: center;
	font-size: 12px;
	color: #666;
}
.colorBule{
	color: #017aff;
}
.share-box,.share-box-app{
	position: relative;
	width: 80%;
	height: 24.5em;
	background: #f3f3f3;
	margin: 4em auto 0em;
}
.wechat-img{
	width:100%;
	margin: 0 auto;
	text-align: center;
	padding: 20px;
	background: #fff;
}
.wechat-img p{
	margin-bottom: 20px;
}
.wechat-img img{
	width: 11.6em;
	height: 11.6em;
}
.other-share-box{
	width: 82%;
	height: 60px;
	padding: 0;
	margin: 0 auto;
}
.other-share-box li{
	width: 20%;
	overflow: hidden;
	
	float: left;
	list-style: none;
	/*margin-left: .9em;*/
	margin-left: 6.5%;
	text-align: center;
}
.other-share-box li:first-child{
	margin-left: 0;
}
.other-share-box li p{
	font-size: .5em;
}
.other-share-box li img{
	width: 2.4em;
	height:2.4em;
}
.share-title{
	width: 82%;
	font-size: .6em;
	margin: 15px auto;
}
.wechat-box-close{
	position: absolute;
	right: 0;
	top: -1.2em;
	color: #fff;
	font-weight: 500;
	
}
.mui-backdrop{
	display: none;
}
.mui-table-view:after,.mui-table-view:before{
	height: 0;
}
